<% if session[:user_id].present? %>
  $('#form').empty();
  $('#nav').empty();
  $('#nav').append('<%=j render :partial => 'layouts/nav' %>');
  $('#flight_whole').empty().append('<%= j render(:partial => 'flights/flights') %>');
<% else %>
  $('#form').empty().append('<%=j render(:partial => 'form') %>');
<% end %>

